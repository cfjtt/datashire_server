package com.eurlanda.datashire.entity;

/**
 * SquidDataRequest
 * 	query all records from a table of any data source
 * 
 * @author dang.lu 2013.09.23
 *
 */
public class SquidTableInfo {
	
	private int squid_id;
	private int current_page; // 当前页码(1..n)
	private int count; // 一页显示多少条记录
	private String table_name;
 
	public int getSquid_id() {
		return squid_id;
	}
	public void setSquid_id(int squid_id) {
		this.squid_id = squid_id;
	}
	public int getCurrent_page() {
		return current_page;
	}
	public void setCurrent_page(int current_page) {
		this.current_page = current_page;
	}
	public int getCount() {
		return count;
	}
	public void setCount(int count) {
		this.count = count;
	}
	public String getTable_name() {
		return table_name;
	}
	public void setTable_name(String table_name) {
		this.table_name = table_name;
	}

	@Override
	public String toString() {
		return "SquidTableInfo [squid_id=" + squid_id + ", current_page="
				+ current_page + ", count=" + count + ", table_name="
				+ table_name + "]";
	}
	
}
