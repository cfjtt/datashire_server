package com.eurlanda.datashire.entity;
/**
 * 导出\导入的列集合
 * @author eurlanda
 *
 */

public class IOFlow {
	
	//SquidFlow中的ID
	private int id;
	//SquidFlow中的名称
	private String name;
	//是否导出全部的squid
	private boolean allSquids;
	//是否导出全部的variables
	private boolean allVariables;
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public boolean isAllSquids() {
		return allSquids;
	}
	public void setAllSquids(boolean allSquids) {
		this.allSquids = allSquids;
	}
	public boolean isAllVariables() {
		return allVariables;
	}
	public void setAllVariables(boolean allVariables) {
		this.allVariables = allVariables;
	}
}
