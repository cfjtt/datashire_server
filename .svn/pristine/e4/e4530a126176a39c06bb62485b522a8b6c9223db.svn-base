package com.eurlanda.datashire.entity.operation;

public class StageData {
	//表名
	private String tableName;
	
	//列集合
	private Object[] sourceColumns;
	
	//列集合
	private Object[] columns;
	
	//Transformations
	private Object[] transformations;
	
	//TransformationLinks
	private Object[] transformationLinks;
	
	//Joins
	private Object[]Joins;

	public StageData() {
		super();
	}

	public StageData(String tableName, Object[] sourceColumns,
			Object[] columns, Object[] transformations,
			Object[] transformationLinks, Object[] joins) {
		super();
		this.tableName = tableName;
		this.sourceColumns = sourceColumns;
		this.columns = columns;
		this.transformations = transformations;
		this.transformationLinks = transformationLinks;
		Joins = joins;
	}

	public String getTableName() {
		return tableName;
	}

	public void setTableName(String tableName) {
		this.tableName = tableName;
	}

	public Object[] getSourceColumns() {
		return sourceColumns;
	}

	public void setSourceColumns(Object[] sourceColumns) {
		this.sourceColumns = sourceColumns;
	}

	public Object[] getColumns() {
		return columns;
	}

	public void setColumns(Object[] columns) {
		this.columns = columns;
	}

	public Object[] getTransformations() {
		return transformations;
	}

	public void setTransformations(Object[] transformations) {
		this.transformations = transformations;
	}

	public Object[] getTransformationLinks() {
		return transformationLinks;
	}

	public void setTransformationLinks(Object[] transformationLinks) {
		this.transformationLinks = transformationLinks;
	}

	public Object[] getJoins() {
		return Joins;
	}

	public void setJoins(Object[] joins) {
		Joins = joins;
	}
	
}
