package com.eurlanda.datashire.utility;

/**
 * 返回状态码的对象，默认返回正确（状态码为1）
 * @author Fumin
 * @version 1.0
 * @created 16-May-2013 5:41:21 PM
 */
public class ReturnValue {

	/**
	 * 返回编号
	 */
	private MessageCode messageCode;

	/**
	 * 构造器
	 */
	public ReturnValue(){
		this.messageCode = MessageCode.SUCCESS;
	}

	/**
	 * 返回编号
	 */
	public MessageCode getMessageCode(){
		return messageCode;
	}

	/**
	 * 返回编号
	 * 
	 * @param newVal
	 */
	public void setMessageCode(MessageCode newVal){
		messageCode = newVal;
	}

}
